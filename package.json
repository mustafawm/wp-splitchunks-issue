    "start": "env-cmd -e development npm run dev -- --env.mode development",
    "start:ci": "env-cmd -e test npm run dev -- --env.mode test",
    "build": "webpack --config webpack/webpack.config.js --env.mode production",
    "build:report": "npm run build -- --env.analyze",
    "build:local": "env-cmd -e localBuild npm run build -- --env.envFile --env.analyze",
